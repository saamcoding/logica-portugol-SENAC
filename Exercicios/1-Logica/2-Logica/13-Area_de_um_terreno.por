programa {
  funcao inicio() {
    
    inteiro largura, comprimento, area

    escreva("Digite a largura do terreno: ")
    leia(largura)

    limpa()

    escreva("Digite o comprimento do terreno: ")
    leia(comprimento)

    limpa()

    area = largura * comprimento

    escreva("A área do terreno é ", area)
  }
}
