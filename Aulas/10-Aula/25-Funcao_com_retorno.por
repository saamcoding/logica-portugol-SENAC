programa {
  /*
  Função com retorno // return

  Temos que declarar o tipo da função, o tipo do retorno 
  da função sendo (inteiro, real, cadeia)
  
  */

  funcao inicio() {
    inteiro num1, num2
    escreva("Digite um número: ")
    leia(num1)
    escreva("Digite um número: ")
    leia(num2)
    escreva(multiplicacao(num1,num2))
  }

  funcao inteiro multiplicacao(inteiro a, inteiro b) {
    retorne a * b
  }
}
